<%- include('../partials/header') %>

<h2>Butter Bundle</h2>

<h1>INDEX PAGE</h1>

<table id="list">
    <thead>
      <tr>
        <th>Exercise Name</th><br>
        <th>Description</th><br>
        <th>Sets</th>
        <th>Reps</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% Exercise.forEach(function(e) { %>
        <tr>
          <td><%= e.exName %></td>
          <td><%= e.description %></td>
          <td><%= e.sets %></td>
          <td><%= e.reps %></td>
          <td><%= e.video %></td>
          <td><a href="/exercise/<%= e._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>





<%- include('../partials/footer') %>